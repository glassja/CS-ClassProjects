<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="450.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.sceneAlarmController">
   <children>
      <Label fx:id="Alarm" alignment="CENTER" layoutX="125.0" layoutY="274.0" prefHeight="53.0" prefWidth="200.0" text="Alarm" textFill="WHITE">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button fx:id="buttonSceneDown" alignment="BOTTOM_CENTER" layoutX="203.0" layoutY="535.0" mnemonicParsing="false" onAction="#shiftSceneDown" style="-fx-background-color: black;" text="↓" textFill="WHITE">
         <font>
            <Font size="24.0" />
         </font></Button>
      <Button fx:id="Hour" layoutX="75.0" layoutY="275.0" mnemonicParsing="false" onAction="#incHour" style="-fx-background-color: black;" text="H" textFill="WHITE">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="Min" layoutX="325.0" layoutY="275.0" mnemonicParsing="false" onAction="#incMin" style="-fx-background-color: black;" text="M" textFill="WHITE">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="alarmSet" alignment="CENTER" layoutX="125.0" layoutY="341.0" mnemonicParsing="false" onAction="#alarmOnOff" prefHeight="51.0" prefWidth="200.0" style="-fx-background-color: black;" text="Set" textFill="WHITE">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Label alignment="CENTER" layoutX="120.0" layoutY="83.0" prefHeight="141.0" prefWidth="210.0" text="⏰" textFill="WHITE">
         <font>
            <Font size="96.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
